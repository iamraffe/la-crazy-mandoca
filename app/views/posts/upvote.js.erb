$("#post_<%= @post.id %> .like a span").text("<%= @post.get_upvotes.size %>").hide().fadeIn(500);
var dislike_button = $("#post_<%= @post.id %> .dislike a span");
if(parseInt(dislike_button.text()) != parseInt("<%= @post.get_downvotes.size %>")){
  $("#post_<%= @post.id %> .dislike a span").text("<%= @post.get_downvotes.size %>").hide().fadeIn(500);
}
